Title: Groups
---
@section Head {    
    <script src="https://cdn.jsdelivr.net/npm/leaflet@1.3.1/dist/leaflet.min.js"></script> 
    <script src="https://cdn.jsdelivr.net/npm/vue2-leaflet@0.0.58/dist/vue2-leaflet.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/leaflet.markercluster@1.3.0/dist/leaflet.markercluster-src.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/leaflet@1.3.1/dist/leaflet.min.css" rel="stylesheet"></link>
    <link href="https://cdn.jsdelivr.net/npm/leaflet.markercluster@1.3.0/dist/MarkerCluster.min.css" rel="stylesheet"></link>
    <link href="https://cdn.jsdelivr.net/npm/leaflet.markercluster@1.3.0/dist/MarkerCluster.Default.min.css" rel="stylesheet"></link>
}

<b-alert variant="warning" show>Due to recent changes in the Meetup.com API, group listings here are limited until a workaround can be found.</b-alert>

<groups></groups>

@section Script {
    <script type="text/javascript">
        Vue.component('v-map', Vue2Leaflet.Map);
        Vue.component('v-tilelayer', Vue2Leaflet.TileLayer);
        Vue.component('v-marker', Vue2Leaflet.Marker);
        Vue.component('v-popup', Vue2Leaflet.Popup);
        Vue.component('v-marker-cluster', httpVueLoader('/components/Vue2LeafletMarkercluster.vue'));

        Vue.component('groups', httpVueLoader('/components/groups/groups.vue'));        
        Vue.component('card-map', httpVueLoader('/components/card-map.vue'));
    </script>
}